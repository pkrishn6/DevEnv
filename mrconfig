[DEFAULT]
sync = 
	git checkout master 
	git fetch upstream
	git merge upstream/master
	git push origin master 

add = 
	       git add .

brStatus = git status .

brcheckout = 
           git checkout -b cavm upstream/cavm
	   git push origin cavm 


push = git push origin master 

